{
    "extends": [
        "react-app",
        "prettier"
    ],
    "env": {
        "browser": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "react/prop-types": 0,
        "import/first": 0,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "react/jsx-no-bind": 0,
        "no-param-reassign": 0,
        "import/no-extraneous-dependencies": 0,
        "arrow-parens": 0,
        "array-callback-return": 0,
        "complexity": 0,
        "react/no-array-index-key": 0,
        "react/sort-comp": 0,
        "class-methods-use-this": 0,
        "no-unused-expressions": 0,
        "consistent-return": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "arrow-body-style": 0,
        "quote-props": 0,
        "no-confusing-arrow": 0,
        "global-require": 0
    }
}