{
    "extends": "dhis2",
    "env": {
        "browser": true,
        "mocha": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-unused-expressions": ["error", { "allowShortCircuit": true }]
    }
}
